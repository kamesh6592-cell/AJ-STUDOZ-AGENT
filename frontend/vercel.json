{
  "version": 2,
  "name": "aj-personal-agent",
  "builds": [
    {
      "src": "api/chat.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ],
  "env": {
    "GROQ_API_KEY": "@groq_api_key",
    "GEMINI_API_KEY": "@gemini_api_key", 
    "ANTHROPIC_API_KEY": "@anthropic_api_key",
    "XAI_API_KEY": "@xai_api_key"
  },
  "buildCommand": "echo 'No build command needed'",
  "outputDirectory": "./",
  "installCommand": "echo 'No install command needed'",
  "framework": null,
  "functions": {
    "api/chat.js": {
      "runtime": "nodejs18.x",
      "memory": 512,
      "maxDuration": 30
    }
  }
}
